{
    "name": "alimianesa/smartauth",
    "description": ":package_description",
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Salimian",
            "email": "ali.salimiansas2@gmail.com",
            "homepage": "https://github.com/alimianesa"
        }
    ],
    "homepage": "https://github.com/alimianesa/smartauth",
    "keywords": ["Laravel", "SmartAuth"],
    "require": {
        "illuminate/support": "~5|~6|~7",
        "teamtnt/laravel-scout-tntsearch-driver": "^8.3",
        "algolia/algoliasearch-client-php": "^2.6",
        "alive2212/laravel-mobile-passport": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Alimianesa\\SmartAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alimianesa\\SmartAuth\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alimianesa\\SmartAuth\\SmartAuthServiceProvider"
            ],
            "aliases": {
                "SmartAuth": "Alimianesa\\SmartAuth\\Facades\\SmartAuth"
            }
        }
    }
}
